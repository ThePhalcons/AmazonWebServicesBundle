{
    "name":         "cybernox/amazon-webservices-bundle",
    "description":  "A Symfony2 Bundle for interfacing with Amazon Web Services (AWS)", 
    "keywords":     ["amazon", "aws", "web services"],
    "license":      "BSD",
    "authors": [
        {
            "name":   "Mark Badolato",
            "email":  "mbadolato@cybernox.com"
        }
    ],  
    "require": {
        "symfony/framework-bundle": "2.*",
        "aws-sdk-for-php": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "aws-sdk-for-php",
                "version": "master",
                "source": {
                    "url": "https://github.com/amazonwebservices/aws-sdk-for-php.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": { "Cybernox\\AmazonWebServicesBundle": "" }
    },  
    "target-dir": "Cybernox/AmazonWebServicesBundle"
}
